{
  "name": "node-template",
  "description": "A basic template for Node applications with linting and Babel transpilation",
  "version": "1.0.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "lib/app.js",
  "scripts": {
    "start": "nodemon src/app.js --exec babel-node --presets latest-minimal",
    "build": "babel lib -d dist",
    "serve": "node dist/app.js",
    "test": "mocha --compilers js:babel-register",
    "lint": "eslint --fix src/ test/"
  },
  "repository": "https://github.com/iknowhtml/node-template.git",
  "author": "Aki Gao",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-latest-minimal": "^1.1.2",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-plugin-node": "^9.1.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1"
  },
  "dependencies": {}
}
